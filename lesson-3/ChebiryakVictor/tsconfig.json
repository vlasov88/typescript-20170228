{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6",
      "dom",
      "es2016.array.include"
    ],
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "noEmitOnError": true,
    "strictNullChecks": false,
    "removeComments": true,
    "declaration": true
  },
  "include": [
    "functions.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}